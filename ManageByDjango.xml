<uwsgi>    
   <socket>127.0.0.1:9999</socket><!-- 内部端口，自定义 --> 
   <chdir>/www/web/api/public_html/</chdir><!-- 项目路径 -->  
   <home>/www/web/api/venv/</home><!-- 虚拟环境路径 -->            
   <module>ManageByDjango.wsgi</module> 
   <processes>4</processes> <!-- 进程数 -->     
   <daemonize>uwsgi.log</daemonize><!-- 日志文件 -->
</uwsgi>